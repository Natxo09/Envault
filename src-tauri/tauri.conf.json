{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Envault",
  "version": "0.2.2",
  "identifier": "com.natxo.envault",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Envault",
        "width": 900,
        "height": 650,
        "minWidth": 600,
        "minHeight": 400,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "shadow": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDdFOTdFODBGNTc5MkQ3MTkKUldRWjE1SlhEK2lYZnVUdG1yZ202VFJkWW1ESXpPT1FvWmc2VVlzZ1RpREdiQUdZRFArOVljM00K",
      "endpoints": [
        "https://github.com/Natxo09/envault/releases/latest/download/latest.json"
      ]
    }
  }
}
